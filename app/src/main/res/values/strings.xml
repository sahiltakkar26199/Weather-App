    <string name="pref_units_key" translatable="false">units</string>
    <string name="pref_units_default_value">@string/pref_units_celcius_value</string>

    <string name="pref_units_celcius_label">Celcius</string>
    <string name="pref_units_kelvin_label">kelvin</string>
    <string name="pref_units_celcius_value" translatable="false">celcius</string>
    <string name="pref_units_kelvin_value" translatable="false">kelvin</string>

