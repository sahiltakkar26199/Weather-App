    <!--or something similar.-->
    <string name="app_name">Sunshine</string>


    <string name="pref_general_home_location_label">Set home Coordinates</string>
    <string name="pref_general_home_location_key">home_coordinates</string>


    <string name="pref_general_latitude_label">Latitude</string>
